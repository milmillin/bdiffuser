{
  "name": "bomb-busters-server",
  "main": "src/worker.ts",
  "compatibility_date": "2024-12-01",
  "observability": {
    "enabled": false,
    "head_sampling_rate": 1,
    "logs": {
      "enabled": true,
      "head_sampling_rate": 1,
      "persist": true,
      "invocation_logs": true
    },
    "traces": {
      "enabled": false,
      "persist": true,
      "head_sampling_rate": 1
    }
  },
  "durable_objects": {
    "bindings": [
      { "name": "BombBustersServer", "class_name": "BombBustersServer" },
      { "name": "StatsServer", "class_name": "StatsServer" }
    ]
  },
  "migrations": [
    { "tag": "v1", "new_sqlite_classes": ["BombBustersServer"] },
    { "tag": "v2", "new_sqlite_classes": ["StatsServer"] },
    { "tag": "v3", "deleted_classes": ["StatsServer"] }
  ]
}
